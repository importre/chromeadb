{
    "app": {
        "background": {
            "scripts": [
                "scripts/background.js"
            ]
        }
    },
    "description": "BrickSimple Chrome ADB (Android Debug Bridge) Client",
    "icons": {
        "128": "assets/icon_128.png",
        "16": "assets/icon_016.png",
        "48": "assets/icon_048.png"
    },
    "manifest_version": 2,
    "minimum_chrome_version": "24",
    "name": "BrickSimple Chrome ADB",
    "permissions": [
        "fileSystem",
        {
            "socket": [
                "tcp-connect"
            ]
        }
    ],
    "version": "0.3.0"
}